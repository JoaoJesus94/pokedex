{
	"printWidth": 120,
	"bracketSpacing": true,
	"useTabs": true,
	"tabWidth": 2,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"importOrder": [
		"^@/api",
		"^@/components",
		"^@/constants",
		"^@/hooks",
		"^@/lib",
		"^@/mocks",
		"^@/public",
		"^@/styles",
		"^@/types",
		"^@/utils"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
